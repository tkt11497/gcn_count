{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "/api/facebook/exchange-token", "function": "exchangeFacebookToken" },
      { "source": "/api/facebook/get-page-tokens", "function": "getPageTokens" },
      { "source": "/api/health", "function": "healthCheck" },
      { "source": "/api/youtube/live-counts", "function": "youtubeLiveCounts" },
      { "source": "**", "destination": "/index.html" }
    ]
  },
  "functions": {
    "source": "functions1"
  }
}
